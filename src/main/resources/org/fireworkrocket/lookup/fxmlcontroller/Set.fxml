         <MFXButton fx:id="checkForUpdates" onAction="#checkForUpdates" prefHeight="79.0" prefWidth="131.0" rippleAnimateShadow="true" rippleColor="WHITE" style="-fx-background-color: rgba(255, 255, 255, 0.5); -fx-background-radius: 0; -fx-border-radius: 0;" text="检查更新" HBox.hgrow="ALWAYS" />
      </children>
   </HBox>
</AnchorPane>
