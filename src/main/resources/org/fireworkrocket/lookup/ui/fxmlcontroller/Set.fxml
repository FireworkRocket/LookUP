<AnchorPane fx:id="SetAnchorPane" prefHeight="394.0" prefWidth="600.0" stylesheets="@test.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fireworkrocket.lookup.ui.fxmlcontroller.SettingConteoller">
